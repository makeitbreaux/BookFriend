{
    "name": "server",
    "version": "0.1.0",
    "main": "server.js",
    "scripts": {
        "installServer": "cd server && npm install",
        "installClient": "cd client && npm install",
        "installAll": "concurrently \"npm run installServer\" \"npm run installClient\"",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "client": "npm start --prefix client",
        "server": "npm run server --prefix server",
        "start": "npm start --prefix server",
        "heroku-prebuild": "cd server && npm install",
        "heroku-postbuild": "cd client && npm install && npm run build"
    },
    "engines": {
        "node": "16.9.1",
        "npm": "7.21.1"
    },
    "jest": {
        "testPathIgnorePatterns": [
            "/node_modules/",
            "config.js"
        ]
    },
    "author": "Briana Breaux",
    "license": "ISC",
    "homepage" : "/homepage",
    "dependencies": {
        "bcrypt": "^5.0.0",
        "body-parser": "^1.19.0",
        "colors": "^1.4.0",
        "concurrently": "^7.1.0",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "jsonschema": "^1.2.6",
        "jsonwebtoken": "^8.5.1",
        "morgan": "^1.10.0",
        "nodemon": "^2.0.15",
        "pg": "^8.3.0",
        "react-scripts": "^5.0.1",
        "supertest": "^5.0.0-0"
    },
    "devDependencies": {
        "concurrently": "^7.1.0"
      }
}
